      // Detect browser language and reset elements on the page.
      let browserLanguage =
        (window && window.navigator && window.navigator.language) || "en";
      let language2 =
        browserLanguage === "ru"
          ? "ru"
          : browserLanguage.substring(browserLanguage.indexOf("-") - 2, 2);
      const language3 =
        languages2[language2] && languages2[language2].language3;
      var langCode = language2;
      var oktaSignIn;
 
      // Update elements based on language
      document.getElementById("headerLogo").src =
        "https://www.churchofjesuschrist.org/services/platform/v3/resources/static/logo/" +
        language3 +
        ".svg";
      document.getElementById("footerLogo").src =
        "https://www.churchofjesuschrist.org/services/platform/v3/resources/static/logo/" +
        language3 +
        ".svg";
 
      function updateText(lang2) {
        let textLang = "en";
        if (lang2 === "zh-CN") {
          textLang = "zhs";
        } else if (lang2.indexOf("-") > 0) {
          textLang = lang2.substring(lang2.indexOf("-") - 2, 2);
        } else {
          textLang = lang2;
        }
        document.getElementById("headerSelectedLang").innerHTML =
          languages2[textLang] && languages2[textLang].language;
        document.getElementById("rights-url").innerHTML =
          languages2[textLang] && languages2[textLang].terms;
        document.getElementById("privacy-url").innerHTML =
          languages2[textLang] && languages2[textLang].privacy;
        document.getElementById("copyright-text").innerHTML =
          languages2[textLang] && languages2[textLang].copyright;
      }
 
      updateText(language2);
 
      // Polyfill for IE 11
      (function (w) {
        w.URLSearchParams =
          w.URLSearchParams ||
          function (searchString) {
            var self = this;
            self.searchString = searchString;
            self.get = function (name) {
              var results = new RegExp("[\?&]" + name + "=([^&#]*)").exec(
                self.searchString
              );
              if (results == null) {
                return null;
              } else {
                return decodeURI(results[1]) || 0;
              }
            };
          };
      })(window);
 
      //capture urls for redirection
      const actualURL = decodeURIComponent(window.location.href);
      const urlParams = new URLSearchParams(window.location.search);
      const redirectUrl = urlParams.get("fromURI");
 
      //get signin widget default configuration
      var config = OktaUtil.getSignInWidgetConfig();
 
      //render signin widget
      function RenderSignInWidget() {
        config = OktaUtil.getSignInWidgetConfig();
        //registration link redirection
        config.registration = {
          click: function () {
            window.location.href =
              "https://account.churchofjesuschrist.org/register";
          },
        };
        //links help
        config.helpLinks = {
          help:
            "https://account.churchofjesuschrist.org/help?lang=" +
            language2 +
            "",
          forgotPassword:
            "https://account.churchofjesuschrist.org/recovery?lang=" +
            language2,
        };
        config.features.securityImage = false;
        config.features.registration = true;
        config.features.hideSignOutLinkInMFA = false;
        config.language = langCode;
      //  config.assets = null;

       config.assets = {
          baseUrl: 'https://op1static.oktacdn.com/assets/js/sdk/okta-signin-widget/3.9.1'
        }
 
        if (oktaSignIn != undefined) {
         oktaSignIn.remove();
       }
        //Object.assign(config, signInWidgetConfig);
        // Render the Okta Sign-In Widget
        //debugger;
        oktaSignIn = new OktaSignIn(config);
       oktaSignIn.renderEl({ el: '#okta-login-container' },
            OktaUtil.completeLogin,
            function(error) {
                // Logs errors that occur when configuring the widget.
                // Remove or replace this with your own custom error handler.
                console.log(error.message, error);
            },
        
          function error(err) {
            // handleError(err)
          }
        );
        oktaSignIn.on("ready", function (context) {
          //debugger;
          //if(context.controller == 'refresh-auth-state')
          //{
          //if(redirectUrl.includes('login/login'))
          //{
          //window.location.href = redirectUrl;
          //}
          //}
        });
        //call after signin widget is rendered
        oktaSignIn.on("afterRender", function (data) {
          //scroll top after rendering to address designing team request/issue
          $(window).scrollTop(0);
 
          // Adjust manual Setup Google Auth Code
          $(".o-form-input-name-sharedSecret input").replaceWith(function () {
            return '<span id="manual-secret-code">' + this.value + "</span>";
          });
 
          //on primary authentication screen
          if (data.controller === "primary-auth") {
            //hide auth header
            $(".okta-sign-in-header.auth-header").hide();
            //display placeholder
            $("#okta-signin-username").attr(
              "placeholder",
              langCode == "en"
                ? "Username"
                : JSON.parse(window.localStorage["osw.languages"])[langCode]
                    .login["primaryauth.username.placeholder"]
            );
          } else if (data.controller === "mfa-verify") {
            //display placeholder
            $(".password-with-toggle").attr(
              "placeholder",
              langCode == "en"
                ? "Password"
                : JSON.parse(window.localStorage["osw.languages"])[langCode]
                    .login["primaryauth.password.placeholder"]
            );
            //display auth header
            $(".okta-sign-in-header.auth-header").show();
 
            //auto click SMS
            var sms_button = $(".sms-request-button")[0];
            if (sms_button) {
              sms_button.click();
            }
 
            //add help links on MFA page
            var pwdNeedHelp = $("#pwdNeedHelp");
            var pwdText = $(
              ".o-form-input-name-password.o-form-control.okta-form-input-field.input-fix"
            );
            if (pwdText.length > 0) {
              if (pwdNeedHelp.length == 0) {
                $(".auth-footer .link.goto").before(
                  '<a href="#" id="pwdNeedHelp" data-se="needhelp" aria-expanded="false" aria-controls="help-links-container" class="link help js-help" >      Need help signing in?      </a>      <ul class="help-links js-help-links" id="help-links-container" style="display: none;">        <li>        <a id= "pwdForgotPasswordNav" href="https://account.churchofjesuschrist.org/help?lang=eng" data-se="forgot-password" class="link js-forgot-password">        Forgot password?        </a>        </li>                        <li>        <a id="pwdHelpNav" href="#" data-se="help-link" class="link js-help-link" target="_blank">Help</a>        </li>      </ul>'
                );
 
                $("#pwdForgotPasswordNav").click(function (e) {
                  var win = window.open(
                    "https://account.churchofjesuschrist.org/recovery?lang=" +
                      language2,
                    "_blank"
                  );
                  if (win) {
                    win.focus();
                  } else {
                    //Browser has blocked it
                    alert("Please allow popups for this website");
                  }
                  e.preventDefault();
                  return false;
                });
                $("#pwdHelpNav").click(function (e) {
                  var win = window.open(
                    "https://account.churchofjesuschrist.org/help?lang=" +
                      language2,
                    "_blank"
                  );
                  if (win) {
                    win.focus();
                  } else {
                    //Browser has blocked it
                    alert("Please allow popups for this website");
                  }
                  e.preventDefault();
                  return false;
                });
                $("#pwdNeedHelp").click(function (e) {
                  if ($("#pwdNeedHelp").attr("aria-expanded") === "false") {
                    $("#help-links-container").show();
                    $("#pwdNeedHelp").attr("aria-expanded", "true");
                  } else {
                    $("#help-links-container").hide();
                    $("#pwdNeedHelp").attr("aria-expanded", "false");
                  }
                  e.preventDefault();
                  return false;
                });
              }
            }
          } else if (data.controller === "enroll-choices") {
            //hide description
            $(".okta-form-subtitle.o-form-explain").hide();
            //hide header
            $(".okta-sign-in-header.auth-header").hide();
            //add signout on enrollment page
            if (document.getElementById("manuallyAddedSignOutLink") === null) {
              $(".o-form.o-form-edit-mode").after(
                '<div class="registration-container" id="manuallyAddedSignOutLink" style="margin-top:0px;"><div class="content-container" style="text-align:right"><a title="Sign Out" aria-label="Sign Out" class="registration-link" href="/login/signout">Sign Out</a></div></div>'
              );
            }
          } else {
            $(".okta-sign-in-header.auth-header").show();
          }
        });
      } 
 
      $(document).ready(function () {
        RenderSignInWidget();
 
        var allOptions = $("#language-options").children("li:not(.init)");
        $("#language-options").on("click", ".init", function () {
          $(this).closest("ul").children("li:not(.init)").toggle();
        });
 
        $("#language-options").on("click", "li:not(.init)", function () {
          allOptions.removeClass("selected");
          $(this).addClass("selected");
          $("#language-options").children(".init").html($(this).html());
          allOptions.toggle();
        });
 
        $("#language-options li").click(function (e) {
          var selLangCode = $(this).attr("langcode");
          langCode = selLangCode;
          language2 = $(this).attr("id");
          config.language = langCode;
 
          //Set header logo and footer logo based on selected language
          $("#headerLogo").attr(
            "src",
            "https://www.churchofjesuschrist.org/services/platform/v3/resources/static/logo/" +
              $(this).attr("id") +
              ".svg"
          );
          $("#footerLogo").attr(
            "src",
            "https://www.churchofjesuschrist.org/services/platform/v3/resources/static/logo/" +
              $(this).attr("id") +
              ".svg"
          );
          $("#rights-url").attr(
            "href",
            "https://www.churchofjesuschrist.org/services/platform/v3/resources/terms-of-use?lang=" +
              language2
          );
          $("#privacy-url").attr(
            "href",
            "https://www.churchofjesuschrist.org/services/platform/v3/resources/terms-of-use?lang=" +
              language2
          );
 
          updateText(langCode);
 
          $("#headerSelectedLang").html($(this).html());
 
          RenderSignInWidget();
        });
      });
 
      //Select language dropdown
      function SelectLanguage() {
        $("#language-options").show();
        var allOptions = $("#language-options").children("li:not(.init)");
        allOptions.toggle();
      }
 
      function getParameterByName(name, url) {
        if (!url) url = window.location.href;
        name = name.replace(/[\[\]]/g, "\\$&");
        var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
          results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return "";
        return decodeURIComponent(results[2].replace(/\+/g, " "));
      }
    </script>
  </body>
</html>
